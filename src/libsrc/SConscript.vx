# scons build file for evio for vxworks

Import('env', 'libInstallDir', 'incInstallDir', 'debugSuffix')

fileList = ['evio_util.c']

env.AppendUnique(CPPPATH = ['.'])
evioLib = env.SharedLibrary(target = 'evio'+debugSuffix, source = fileList)

env.Alias("install", env.Install(target = incInstallDir, source = ['evio_util.h']))
env.Alias("install", env.Install(target = libInstallDir, source = evioLib))
